<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_with(model: movie) do |f| %>
      <%= f.label :name %>
      <%= f.text_field :name%>

      <%= f.label :cinema_id %>
      <%= f.select :cinema_id, @cinemas.collect {|cinema| [cinema.name, cinema.id]} %>

      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>